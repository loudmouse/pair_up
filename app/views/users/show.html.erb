
<h1>Hosting</h1>
  <table>
    <tbody>
      <tr>
        <strong>Description:</strong>
      </tr>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.description %></td>
      </tr>
    <% end %>
    </tbody>
  </table>

  <h2>Attending</h2>

  <% @user.attended_events.each do |attended_event| %>
  	<li class="list-group-item">
  		<%= link_to attended_event.description, attended_event %>
  	</li>
  <% end %>
